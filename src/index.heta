/*
  Minimal heta file
*/
namespace one begin
  comp1 @Compartment .= 1;
  A @Species { compartment: comp1 };
  B @Species { compartment: comp1 };
  r1 @Reaction { actors: A => 2B };

  // init
  A .= 10;
  B .= 0;
  // rhs
  r1 := k1*A*comp1;
end

k1 @Const = 1.2e-1;

sbml @SBMLExport { model: one };
mrg @MrgsolveExport { model: one };

slv @SLVExport { model: one};
simbio @SimbioExport { model: one};
json @JSONExport;
yaml @YAMLExport;
